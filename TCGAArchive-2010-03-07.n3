                owl:onProperty tcga:has_last_modified_date ;
                owl:someValuesFrom tcga:ModificationDateTime
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:onProperty tcga:has_file_type ;
                owl:someValuesFrom tcga:ArchiveFileType
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Class ;
                owl:unionOf ([ rdf:type owl:Restriction ;

                            owl:someValuesFrom tcga:URI
                          ] [ rdf:type owl:Restriction ;
                            owl:onProperty tcga:has_location ;
                            owl:someValuesFrom tcga:FileLocation
                          ])
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:onProperty tcga:has_archive ;
                owl:someValuesFrom tcga:Archive
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:onProperty mage:has_name ;
                owl:someValuesFrom xsd:string
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:onProperty tcga:has_file_type ;
                owl:someValuesFrom tcga:ArchiveFileType
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:onProperty tcga:has_last_modified_date ;
                owl:someValuesFrom xsd:string
              ] .


tcga:Cell
      rdf:type owl:Class ;
      rdfs:comment "This is an alternative perspective of a Cell; a class that allows further extension (and variation) of Cell outside of a MAGE or TCGA perspective. This class is equivalent to the MAGE BioMaterial class that has a has_type property with a MAGE MaterialType of Cell."^^xsd:string ;
      rdfs:subClassOf tcga:BioMaterial ;
      tcga:has_Term tcga:Célula ;
      mage:has_type mage:cell ;
      owl:equivalentClass
              [ rdf:type owl:Restriction ;
                owl:hasValue mage:cell ;
                owl:onProperty mage:has_type
              ] .

